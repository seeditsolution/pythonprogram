def fib(n):

    if n<0:
        print("value is less than 0,you entered wrong input")

    else:
        a=0
        b=1
        if (n==1) or (n==0):
            print(a)

        else:
            print(a)
            print(b)
            for i in range(2,n):
                c= a+b
                a=b
                b=c
                print(c)

fib(int(input("Enter the number you want to find fibonaci sequence for :")))



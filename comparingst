if __name__ == "__main__" :  
      
    # Get the strings which  
    # is to be checked  
    string1 = input("Enter the first string: ") 
    print(string1, end = "\n") 
      
    # Get the strings which  
    # is to be checked  
    string2 = input("Enter the second string: ") 
    print(string2, end ="\n") 
      
    # Check if both strings are equal  
    print("Are both strings same: ", end = " ")  
  
    if (string1 == string2) :  
        print("Yes") 
          
    else :  
        print("No")  

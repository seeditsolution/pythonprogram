yr=int(input('enter year'))
if yr%100==0: #century year
       if yr%400==0:
           print ('{} is leap year'.format(yr))
       else:
          print ('{} is not leap year'.format(yr))
else:
       if yr%4==0:
          print ('{} is leap year'.format(yr))
else:
         print ('{} is not leap year'.format(yr))

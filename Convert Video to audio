import moviepy.editor as mp     #[at first install moviepy in ur system]

clip = mp.VideoFileClip(r'C:\\Users\\ADMIN\\Desktop\\Despacito.mp4')         #[ Copy and paste the path where u saved your video]

clip.audio.write_audiofile('C:\\Users\\ADMIN\\Desktop\\Despacito_cvrt.mp3')  #[ Give the path where You want to save your audio file]

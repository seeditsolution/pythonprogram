

a=10
b=40
c=30

if(a<b) & (b>c):
    print ("b is greatest")
elif(a>b) & (b<c):
    print ("a is greatest")
else:
    print ("c is greatest")



#if with tuple

tup1=("a","b","c")

if "z" in tup1:
    print("z is present in tup1")
else:
    print("z is not present in tup1")

#if with dictionary
d1={'k1':20,'k2':30, 'k3':40}



d1

if d1["k3"]==40:
    d1["k3"]=d1["k2"]+100   

d1


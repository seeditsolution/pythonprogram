while i<=20:
    print(i)
    i=i+2

i=30
while i<=300:
    print(i)
    i=i+30



i

i=2
n=2
while i<=20:
    print(n ," * ",i," = ",n*i)


i=1
n=2
while i=20:
    print(n ," * ",i," = ",n*i)
    i=i+1









